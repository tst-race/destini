{
    "android_path": "scripts/jel2_Pixelfed_codec_android.sh",
    "path": "scripts/jel2_decomposed_codec_p3.sh",

    "args": {
        "common": "-seed <secret> -bpf 1 -nfreqs 1 -maxfreqs 6",
        "encode": "<coverfile>"
    },

    "media": {
        "wcap_path": "bin/wcap",
        "capacities": "jpeg-covers/capacities.json",
        "maximum": 3000
    },

    "initCommand": "/bin/bash -c scripts/initialize_jel2.sh",

    ".format": "Decomposed-TA2",
    "mime-type": "image/jpeg",
    "encodingTime": 0.5
}
